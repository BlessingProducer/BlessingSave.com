---
import Layout from '../layouts/Layout.astro';
import App from '../App'
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import Scrollbar from "../components/Scrollbar.astro";
import SEOContainer from "../components/SEOContainer.astro";

const props = {
    formTitle: "Tiktok Downloader",
    formDescription: "Download Tiktok Video to Mp4",
    formType: "tt",
    formPlaceholder: "Enter Tiktok link here",
    formPasteButtonTitle: "Paste",
    formClearButtonTitle: "Clear",
    formSubmitButtonTitle: "Download",
}
---
<Layout
        site="https://blessingsave.com"
        title={"Tiktok Video Downloader"}
        description={"Tiktok Video Download, Photos, Reels, Stories to Mobile and PC"}
>
    <Scrollbar>
        <Header
                tiktokButtonTitle="Tiktok Downloader"
                facebookButtonTitle="Facebook Downloader"
                instagramButtonTitle="Instagram Downloader"
                languageButtonTitle="Language"
        />
        <App
                client:load {...props}
        />
        <SEOContainer
                introduceTitle=""
                introduceContent=""
                featuresTitle=""
                featuresContents={[]}
                FAQTitle=""
                FAQItems={[]}
        />
        <Footer/>
    </Scrollbar>
</Layout>
